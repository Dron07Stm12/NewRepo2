<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Scb_Electronmash.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Scb_Electronmash"
    Title="Scb_Electronmash">


    <FlyoutItem Title="Меню">
        <ShellContent Title="Меню" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage"/>
    </FlyoutItem>
    <FlyoutItem Title="Настройки">
        <ShellContent Title="Настройки" ContentTemplate="{DataTemplate local:Settings2Page}" Route="Settings2Page"/>
    </FlyoutItem>



</Shell>
